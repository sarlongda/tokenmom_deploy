              Trade.order_alert();
              return Trade.get_open_orders();
            }
          }
        } else if (type === "remove") {
          if (symbol === order.token_symbol) {
            Trade.get_orders(order.token_symbol);
            return Trade.get_open_orders();
          }
        } else if (type === "update") {
          if (symbol === order.token_symbol) {
            Trade.get_orders(order.token_symbol);
            return Trade.get_open_orders();
          }
        }
      },
      create_order: function(data) {}
    });
  });

}).call(this);
