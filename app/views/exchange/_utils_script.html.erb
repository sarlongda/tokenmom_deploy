        decimalPlaces:function(n){                                 
            //var numAsStr = n.toFixed(11);
            var numAsStr = n.toString();            
            numAsStr = numAsStr.replace(/0+$/g,'');
            var precision = String(numAsStr).replace('.','').length - parseInt(parseFloat(n)/1).toFixed().length;
            return precision;
        },
