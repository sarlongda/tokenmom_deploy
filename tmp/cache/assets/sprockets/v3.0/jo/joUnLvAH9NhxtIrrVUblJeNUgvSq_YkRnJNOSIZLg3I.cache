    $messages.html('');
    $.each(data, function(index, element) {
      return $messages.append('<div class="message">' + '<strong><a href="' + http_link + 'address/' + element.contract_address + '" rel="noopener noreferrer" target="_blank">' + element.nick_name + ' :</a></strong> ' + element.content + '<br>' + '</div>');
    });
  };

  getMessages = function(param1) {
    param1 = param1 || 1;
    return $.ajax({
