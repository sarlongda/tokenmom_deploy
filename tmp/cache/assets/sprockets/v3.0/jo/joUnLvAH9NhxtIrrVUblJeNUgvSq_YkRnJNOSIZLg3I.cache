        message_body = $new_message_body.val();
        if (e.keyCode === 13 && $.trim(message_body).length > 0 && $.trim(message_body).length < 100) {
          App.chat.send_message(message_body);
        }
        e.preventDefault();
        return false;
      }));
    }
  });

  drawMessages = function(data) {
    var $messages;
